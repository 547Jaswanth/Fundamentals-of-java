import java.util.Scanner;
public class Pow
{
    public static void main(String args[])
    {
        //write your code
        Scanner sc=new Scanner(System.in);
        int b,p;
        b=sc.nextInt();
        p=sc.nextInt();
        double x=Math.pow(b,p);
    
        System.out.printf("%.0f",x);
        
    }
}
